{
  "firestore": {
    "database": "(default)",
    "location": "us-central1", 
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=31536000,immutable"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "pegas-pos",
          "appId": "1:307020637467:android:d884d9a8b99a8c3e28dfbe",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "pegas-pos",
          "configurations": {
            "android": "1:307020637467:android:d884d9a8b99a8c3e28dfbe",
            "ios": "1:307020637467:ios:faeb7158e0abad3d28dfbe",
            "macos": "1:307020637467:ios:faeb7158e0abad3d28dfbe",
            "web": "1:307020637467:web:94889a3fa0fbf5fa28dfbe",
            "windows": "1:307020637467:web:25f4ee879ed817b228dfbe"
          }
        }
      }
    }
  }
}